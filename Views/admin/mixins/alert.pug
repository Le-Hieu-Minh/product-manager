mixin alert-success(time)
  - if (messages.success)
    .message.success
      .alert.alert-success(show-alert data-time=time) #{messages.success}
       span(close-alert) x 



mixin alert-error(time)
  - if (messages.error)
    .message.error
      .alert.alert-danger(show-alert data-time=time) #{messages.error}
       span(close-alert) x 
